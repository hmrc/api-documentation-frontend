              "type" : "{\n  \"title\": \"HateoasCheckCasesWithAgents\",\n  \"type\": \"object\",\n  \"additionalProperties\": false,\n  \"required\": [\n    \"_links\",\n    \"submittedBy\",\n    \"createdDateTime\",\n    \"uarn\",\n    \"address\",\n    \"checkCaseStatus\",\n    \"checkCaseReference\",\n    \"checkCaseSubmissionId\"\n  ],\n  \"properties\": {\n    \"_links\": {\n      \"type\": \"object\",\n      \"additionalProperties\": false,\n      \"properties\": {\n        \"self\": {\n          \"type\": \"object\",\n          \"required\": [\n            \"template\",\n            \"href\"\n          ],\n          \"properties\": {\n            \"href\": {\n              \"type\": \"string\",\n              \"format\": \"uri\",\n              \"pattern\": \"^https?://\\\\w+\",\n              \"description\": \"URL for this link\"\n            },\n            \"template\": {\n              \"type\": \"boolean\",\n              \"description\": \"Is the HREF a RFC-6570 compliant URI Template\"\n            }\n          }\n        }\n      }\n    },\n    \"address\": {\n      \"type\": \"string\",\n      \"description\": \"The address of the property\"\n    },\n    \"agent\": {\n      \"type\": \"object\",\n      \"description\": \"Details of the current agent assigned to the property.\",\n      \"required\": [\n        \"organisationName\",\n        \"code\",\n        \"organisationId\"\n      ],\n      \"properties\": {\n        \"code\": {\n          \"type\": \"number\",\n          \"format\": \"number\",\n          \"description\": \"The unique code of the agent\"\n        },\n        \"organisationId\": {\n          \"type\": \"number\",\n          \"format\": \"number\",\n          \"description\": \"Unique ID for the agent organisation\"\n        },\n        \"organisationName\": {\n          \"type\": \"string\",\n          \"description\": \"The name of the agent organisation\"\n        }\n      },\n      \"additionalProperties\": false\n    },\n   