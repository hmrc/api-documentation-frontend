                  "type" : "{\n  \"$schema\": \"http://json-schema.org/draft-07/schema#\",\n  \"description\": \"My Organisations Property Link (summary)\",\n  \"type\": \"object\",\n  \"additionalProperties\": false,\n  \"definitions\": {},\n  \"required\": [\n    \"propertyLink\",\n    \"_links\"\n  ],\n  \"properties\": {\n    \"_links\": {\n      \"type\": \"object\",\n      \"additionalProperties\": false,\n      \"description\": \"HATEOAS links\",\n      \"properties\": {\n        \"self\": {\n          \"type\": \"object\",\n          \"additionalProperties\": false,\n          \"description\": \"Links to this resource\",\n          \"required\": [\n            \"href\",\n            \"template\"\n          ],\n          \"properties\": {\n            \"href\": {\n              \"type\": \"string\",\n              \"format\": \"uri\",\n              \"pattern\": \"^https?://\\\\w+\",\n              \"description\": \"URL for this link\"\n            },\n            \"template\": {\n              \"type\": \"boolean\",\n              \"description\": \"Is the HREF a RFC-6570 compliant URI Template\"\n            }\n          }\n        }\n      }\n    },\n    \"propertyLink\": {\n      \"type\": \"object\",\n      \"additionalProperties\": false,\n      \"required\": [\n        \"agents\",\n        \"localAuthorityRef\",\n        \"address\",\n        \"uarn\",\n        \"startDate\",\n        \"status\",\n        \"submissionId\",\n        \"id\"\n      ],\n      \"properties\": {\n        \"address\": {\n          \"type\": \"string\",\n          \"description\": \"The address of the linked property\"\n        },\n        \"agents\": {\n          \"type\": \"array\",\n          \"description\": \"The list of agents assigned to represent this property\",\n          \"items\": {\n            \"type\": \"object\",\n            \"additionalProperties\": false,\n            \"required\": [\n              \"organisationName\",\n              \"organisationId\"\n            ],\n            \"properties\": {\n              \"organisationId\": {\n                \"type\": \"number\",\n                \"description\": \"The agent's organisation ID\"\n              },\n              \"organisationName\": {\n                \"type\": \"string\",\n                \"description\": \"The agent's organisation name\"\n              }\n            }\n          }\n        },\n        \"id\": {\n          \"type\": \"integer\",\n          \"description\": \"Unique ID for this property link\"\n        },\n        \"localAuthorityRef\": {\n          \"type\": \"string\",\n          \"description\": \"The local authority reference number for the linked property\"\n        },\n        \"status\": {\n          \"type\": \"string\",\n          \"description\": \"Status of this property link\",\n          \"enum\": [\n            \"APPROVED\",\n            \"PENDING\",\n            \"DECLINED\",\n            \"REVOKED\",\n            \"MORE_EVIDENCE_REQUIRED\"\n          ]\n        },\n        \"startDate\": {\n          \"type\": \"string\",\n          \"format\": \"date\",\n          \"description\": \"The date from which the property link is active\"\n        },\n        \"endDate\": {\n          \"type\": \"string\",\n          \"format\": \"date\",\n          \"description\": \"The date until which the property link is active\"\n        },\n        \"submissionId\": {\n          \"type\": \"string\",\n          \"description\": \"Unique submission ID for this property link\"\n        },\n        \"uarn\": {\n          \"type\": \"integer\",\n          \"description\": \"The UARN (Unique Address Reference Number) of the linked property\"\n        }\n      }\n    }\n  }\n}\n",
