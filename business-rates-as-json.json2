            "type" : "{\n  \"$schema\": \"http://json-schema.org/draft-07/schema#\",\n  \"description\": \"Challenge Case Schema\",\n  \"type\": \"object\",\n  \"additionalProperties\": false,\n  \"title\": \"Challenge case submission request\",\n  \"properties\": {\n    \"valuationId\": {\n      \"description\": \"Unique valuation reference number\",\n      \"type\": \"integer\",\n      \"maximum\": 9223372036854775807\n    },\n    \"checkCaseReference\": {\n      \"description\": \"Check case reference that relates to the challenge\",\n      \"type\": \"string\",\n      \"minLength\": 1\n    },\n    \"propertyLinkSubmissionId\": {\n      \"description\": \"Submission ID of the property link that the challenge relates to\",\n      \"type\": \"string\",\n      \"minLength\": 1\n    },\n    \"reasonCode\": {\n      \"description\": \"Reason for challenge case. Uses a lookup code.\",\n      \"lookup\": \"challengeCodeOfGroundsDfe\",\n      \"type\": \"string\",\n      \"minLength\": 1\n    },\n    \"proposedRateableValue\": {\n      \"description\": \"The value you want to propose as the property's rateable value, the propertys rateable value is the estimate of the yearly rent it could have been let for on a date set in law (currently 1 April 2015). It might not be the actual rent as there are other factors to consider\",\n      \"type\": \"integer\",\n      \"minimum\": 0,\n      \"maximum\": 999999999\n    },\n    \"proposedEffectiveDate\": {\n      \"description\": \"When should your changes take effect? This date cannot be before the list of business rates valuations was published (currently 1 April 2017).\",\n      \"type\": \"string\",\n      \"format\": \"date\"\n    },\n    \"attachments\": {\n      \"description\": \"File attachments supporting the case\",\n      \"type\": \"array\",\n      \"minItems\": 2,\n      \"maxItems\": 10,\n      \"items\": {\n        \"$ref\": \"#/definitions/attachment\"\n      }\n    },\n    \"challengeRentalData\": {\n      \"$ref\": \"#/definitions/challengerentaldata\"\n    },\n    \"groupPreChallengeReviewId\": {\n      \"description\": \" A group pre-challenge review (GPCR) is an option offered by the Valuation Office Agency (VOA) to groups of property occupiers, owners or ratepayers of business and other non-domestic properties. It allows them to challenge the basis of the valuation in respect of local, regional or national schemes of valuation.  \",\n      \"type\": \"integer\",\n      \"minimum\": 1,\n      \"maximum\": 99999999\n    },\n    \"declaration\": {\n      \"description\": \"I declare that the information I have given is correct. If the information provided during a challenge is incorrect, this could lead to a penalty. The rateable value could also change, leading to a backdated business rates bill.\",\n      \"type\": \"boolean\",\n      \"enum\": [\n        true\n      ]\n    }\n  },\n  \"required\": [\n    \"valuationId\",\n    \"checkCaseReference\",\n    \"propertyLinkSubmissionId\",\n    \"reasonCode\",\n    \"proposedEffectiveDate\",\n    \"attachments\",\n    \"declaration\"\n  ],\n  \"definitions\": {\n    \"attachment\": {\n      \"description\": \"File attachment\",\n      \"type\": \"object\",\n      \"additionalProperties\": false,\n      \"properties\": {\n        \"filename\": {\n          \"description\": \"File name\",\n          \"type\": \"string\",\n          \"minLength\": 1\n        },\n        \"fileType\": {\n          \"$ref\": \"#/definitions/filetype\"\n        }\n      },\n      \"title\": \"Attachment\",\n      \"required\": [\n        \"filename\",\n        \"fileType\"\n      ]\n    },\n    \"challengerentaldata\": {\n      \"type\": \"object\",\n      \"additionalProperties\": false,\n      \"properties\": {\n        \"amountOfRentPayable\": {\n          \"description\": \"How much is the annual rent\",\n          \"type\": \"number\",\n          \"minimum\": 1,\n          \"maximum\": 999999999.99\n        },\n        \"timePeriodOfRentPayable\": {\n          \"description\": \"The rental period\",\n          \"type\": \"string\",\n          \"enum\": [\n            \"ANNUALLY\",\n            \"MONTHLY\",\n            \"WEEKLY\"\n          ]\n        },\n        \"startDateOfRentPayable\": {\n          \"description\": \"This cannot be a future date. If you are in a rent-free period, enter the date the agreement started\",\n          \"type\": \"string\",\n          \"format\": \"date\"\n        },\n        \"rentFreeMonths\": {\n          \"description\": \"If the lease includes a rent-free period, how long is it (in months)\",\n          \"type\": \"integer\",\n          \"minimum\": 1,\n          \"maximum\": 999\n        }\n      },\n      \"required\": [\n        \"amountOfRentPayable\",\n        \"timePeriodOfRentPayable\",\n        \"startDateOfRentPayable\"\n      ]\n    },\n    \"filetype\": {\n      \"description\": \"Type of file attachment\",\n      \"type\": \"string\",\n      \"enum\": [\n        \"EVIDENCE\",\n        \"STATEMENT\"\n      ]\n    }\n  }\n}",
            "required" : true,
            "examples" : [ {
              "value" : "{\n  \"valuationId\": 1,\n  \"checkCaseReference\": \"CHK123\",\n  \"propertyLinkSubmissionId\": \"PLZ60Y\",\n  \"reasonCode\": \"41a\",\n  \"proposedRateableValue\": 1234,\n  \"proposedEffectiveDate\": \"2018-09-03\",\n  \"attachments\": [\n    {\n      \"filename\": \"file1\",\n      \"fileType\": \"EVIDENCE\"\n    },\n    {\n      \"filename\": \"file1\",\n      \"fileType\": \"STATEMENT\"\n    }\n  ],\n  \"challengeRentalData\": {\n    \"amountOfRentPayable\": 123.23,\n    \"timePeriodOfRentPayable\": \"ANNUALLY\",\n    \"startDateOfRentPayable\": \"2018-09-03\",\n    \"rentFreeMonths\": 3\n  },\n  \"groupPreChallengeReviewId\": 1,\n  \"declaration\": true\n}"
