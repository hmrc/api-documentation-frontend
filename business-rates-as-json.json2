     },\n        \"fileName\": {\n          \"type\": \"string\",\n          \"description\": \"The provided filename\"\n        },\n        \"history\": {\n          \"type\": \"array\",\n          \"description\": \"The history of the attachment\",\n          \"items\": {\n            \"type\": \"object\",\n            \"additionalProperties\": false,\n            \"required\": [\n              \"timeStamp\",\n              \"state\"\n            ],\n            \"properties\": {\n              \"state\": {\n                \"type\": \"string\",\n                \"description\": \"The state of evidence upload\"\n              },\n              \"timeStamp\": {\n                \"type\": \"string\",\n                \"format\": \"date-time\",\n                \"description\": \"The time stamp of change\"\n              }\n            }\n          }\n        },\n        \"mimeType\": {\n          \"type\": \"string\",\n          \"description\": \"The provided mime-type\"\n        },\n        \"state\": {\n          \"type\": \"string\",\n          \"description\": \"The current state of evidence upload\"\n        }\n      }\n    }\n  }\n}",
