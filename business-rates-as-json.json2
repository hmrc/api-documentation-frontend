                      "type" : "{\n  \"$schema\": \"http://json-schema.org/draft-07/schema#\",\n  \"description\": \"My Organisation's Property Link Evidence attachment\",\n  \"type\": \"object\",\n  \"additionalProperties\": false,\n  \"definitions\": {},\n  \"required\": [\n    \"evidence\",\n    \"_links\"\n  ],\n  \"properties\": {\n    \"_links\": {\n      \"type\": \"object\",\n      \"additionalProperties\": false,\n      \"properties\": {\n        \"self\": {\n          \"type\": \"object\",\n          \"additionalProperties\": false,\n          \"properties\": {\n            \"href\": {\n              \"type\": \"string\",\n              \"format\": \"uri\",\n              \"pattern\": \"^https?://\\\\w+\",\n              \"description\": \"URL for this link\"\n            },\n            \"template\": {\n              \"type\": \"boolean\",\n              \"description\": \"Is the HREF a RFC-6570 compliant URI Template\"\n            }\n          },\n          \"required\": [\n            \"href\",\n            \"template\"\n          ]\n        }\n      }\n    },\n    \"evidence\": {\n      \"type\": \"object\",\n      \"additionalProperties\": false,\n      \"required\": [\n        \"history\",\n        \"state\",\n        \"data\",\n        \"destination\",\n        \"mimeType\",\n        \"fileName\"\n      ],\n      \"properties\": {\n        \"data\": {\n          \"type\": \"object\",\n          \"additionalProperties\": false,\n          \"description\": \"The additional data provided with this piece of evidence\",\n          \"required\": [\n            \"propertyLinkSubmissionId\"\n          ],\n          \"properties\": {\n            \"propertyLinkSubmissionId\": {\n              \"type\": \"string\",\n              \"description\": \"The property link submission ID\"\n            }\n          }\n        },\n        \"destination\": {\n          \"type\": \"string\",\n          \"description\": \"The destination for upload e.g PROPERTY_LINKING\"\n        },\n        \"fileName\": {\n          \"type\": \"string\",\n          \"description\": \"The provided filename\"\n        },\n        \"history\": {\n          \"type\": \"array\",\n          \"description\": \"The history of the attachment\",\n          \"items\": {\n            \"type\": \"object\",\n            \"additionalProperties\": false,\n            \"required\": [\n              \"timeStamp\",\n              \"state\"\n            ],\n            \"properties\": {\n              \"state\": {\n                \"type\": \"string\",\n                \"description\": \"The state of evidence upload\"\n              },\n              \"timeStamp\": {\n                \"type\": \"string\",\n                \"format\": \"date-time\",\n                \"description\": \"The time stamp of change\"\n              }\n            }\n          }\n        },\n        \"mimeType\": {\n          \"type\": \"string\",\n          \"description\": \"The provided mime-type\"\n        },\n        \"state\": {\n          \"type\": \"string\",\n          \"description\": \"The current state of evidence upload\"\n        }\n      }\n    }\n  }\n}",
