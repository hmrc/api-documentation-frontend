            "type" : "{\n  \"description\": \"Attach Property Link Evidence Request\",\n  \"type\": \"object\",\n  \"additionalProperties\": false,\n  \"required\": [\n    \"fileName\",\n    \"mimeType\",\n    \"successRedirect\",\n    \"errorRedirect\"\n  ],\n  \"properties\": {\n    \"fileName\": {\n      \"type\": \"string\",\n      \"minLength\": 1,\n      \"maxLength\": 255,\n      \"description\": \"The name of the file to be submitted as evidence\"\n    },\n    \"mimeType\": {\n      \"type\": \"string\",\n      \"minLength\": 1,\n      \"description\": \"The MIME type of the file being submitted\"\n    },\n    \"successRedirect\": {\n      \"type\": \"string\",\n      \"format\":  \"uri\",\n      \"description\": \"URL you will be redirected to following a successful file upload\"\n    },\n    \"errorRedirect\": {\n      \"type\": \"string\",\n      \"format\":  \"uri\",\n      \"description\": \"URL you will be redirected to, should the file upload fail for any reason\"\n    }\n  }\n}",
