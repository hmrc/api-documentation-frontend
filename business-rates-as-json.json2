            "type" : "{\n  \"description\": \"Create Client Property Link Request\",\n  \"type\": \"object\",\n  \"additionalProperties\": false,\n  \"required\": [\n    \"clientOrganisationId\",\n    \"uarn\",\n    \"capacity\",\n    \"providedEvidence\",\n    \"evidence\",\n    \"startDate\"\n  ],\n  \"properties\": {\n    \"clientOrganisationId\": {\n      \"description\": \"Unique ID for the client organisation\",\n      \"type\": \"integer\",\n      \"maximum\": 9223372036854775807\n    },\n    \"uarn\": {\n      \"description\": \"The UARN (Unique Address Reference Number) of the property\",\n      \"type\": \"integer\",\n      \"maximum\": 9223372036854775807\n    },\n    \"capacity\": {\n      \"type\": \"string\",\n      \"enum\": [\n        \"OWNER\",\n        \"OCCUPIER\",\n        \"OWNER_OCCUPIER\"\n      ],\n      \"description\": \"The capacity of the organisation creating the link\"\n    },\n    \"providedEvidence\": {\n      \"type\": \"string\",\n      \"enum\": [\n        \"RATES_BILL\",\n        \"OTHER\"\n      ],\n      \"description\": \"The type of evidence provided in the evidence array\"\n    },\n    \"evidence\": {\n      \"type\": \"array\",\n      \"minItems\": 1,\n      \"maxItems\": 2,\n      \"items\": {\n        \"description\": \"Evidence attached to support the property link request\",\n        \"type\": \"object\",\n        \"additionalProperties\": false,\n        \"required\": [\n          \"evidenceType\",\n          \"fileName\"\n        ],\n        \"properties\": {\n          \"evidenceType\": {\n            \"type\": \"string\",\n            \"description\": \"The type of evidence\",\n            \"enum\": [\n              \"lease\",\n              \"otherUtilityBill\",\n              \"ratesBill\",\n              \"serviceCharge\",\n              \"waterRateDemand\",\n              \"stampDutyLandTaxForm\",\n              \"license\",\n              \"landRegistryTitle\",\n              \"otherSupportingDocument\"\n            ]\n          },\n          \"fileName\": {\n            \"description\": \"The file name of the evidence\",\n            \"type\": \"string\",\n            \"minLength\": 1,\n            \"maxLength\": 255\n          }\n        }\n      }\n    },\n    \"startDate\": {\n      \"type\": \"string\",\n      \"description\": \"The start date as a ISO Date String\"\n    },\n    \"endDate\": {\n      \"type\": \"string\",\n      \"description\": \"The end date as a ISO Date String. The End date must be at least one calendar day after the Start date.\"\n    }\n  }\n}",
